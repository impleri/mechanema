{
  "extends": [
    "airbnb-base",
    "plugin:jest/recommended",
    "plugin:flowtype/recommended"
  ],
  "env": {
    "jest/globals": true
  },
  "plugins": [
    "jest",
    "flowtype"
  ],
  "rules": {
    "import/no-extraneous-dependencies": "warn"
  },
  "settings": {
    "flowtype": {
      "onlyFilesWithFlowAnnotation": true
    }
  }
}
